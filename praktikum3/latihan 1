a=float(input("masukan panjang sisi satu:"))
b=float(input("masuka panjang sisi dua:"))
c=float(input("masukan panjang sisi tiga:"))
#masukan rumus
p=(c)**2
q=(a)**2+(b)**2
if(p==q):
    print("merupakan segitiga siku siku")
elif(a==b==c):
    print("merupakan segitiga sama sisi")
elif((a+b<=c) or (a+c<=b) or (b+c<=a)):
    print("merupakan bukan segitiga ")
elif((a==b) or (a==c )or (b==c)):
    print("merupakan segitiga sama kaki")
elif (a!=b!=c):
    print("merupakan segitiga sembarang")
